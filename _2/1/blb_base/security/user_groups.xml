<odoo>
    <data noupdate="1">

    <record model="ir.module.category" id="module_category_blb">
      <field name="name">BLB - User Roles</field>
      <field name="description">User groups/roles for BLB modules</field>
      <field name="sequence">1</field>
    </record>
   
   <!-- Branches  -->
    <record id="blb_branch" model="res.groups">
      <field name="name">Branch</field>
      <field name="category_id" ref="module_category_blb"/>
    </record>

    <record id="blb_branch_user" model="res.groups">
      <field name="name">Branch Officer</field>
      <field name="implied_ids" eval="[(4, ref('blb_base.blb_branch'))]"/>
      <field name="category_id" ref="module_category_blb"/>
    </record>
	
   <!-- Client Management/Customer Care -->  
	 <record id="blb_customercare" model="res.groups">
      <field name="name">Client Management</field>
      <field name="category_id" ref="module_category_blb"/>
    </record>

    <record id="blb_customercare_user" model="res.groups">
