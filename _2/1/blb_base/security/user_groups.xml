    <!-- Administrator -->
     <record id="blb_administrator" model="res.groups">
        <field name="name">BLB Administrator</field>
        <field name="implied_ids" eval="[(4, ref('blb_base.blb_branch')),
			    (4, ref('blb_base.blb_branch_user')),
			    (4, ref('blb_base.blb_customercare')),
          (4, ref('blb_base.blb_customercare_user')),
          (4, ref('blb_base.blb_legal')),
          (4, ref('blb_base.blb_legal_user')),
          (4, ref('blb_base.blb_registry')),
